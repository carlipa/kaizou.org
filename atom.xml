---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Kaizou</title>
  <link href="http://www.kaizou.org/"/>
  <link type="application/atom+xml" rel="self" href="http://www.kaizou.org/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.kaizou.org/</id>
  <author>
    <name>David Corvoysier</name>
    <email>kaizouman@kaizou.org</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://www.kaizou.org{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://kaizou.org{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>David Corvoysier</name>
      <uri>http://www.kaizou.org/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
